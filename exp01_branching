
import java.util.*;

public class exp_01_branch_loop {

    public static void main(String args[]) {

        Scanner sc = new Scanner(System.in);

        System.out.print("Enter the Number");
        int num = sc.nextInt();

        for (int i = 0; i <= num; i++) {
            if (i % 2 == 0) {
                System.out.print("The Even Number:" + i + "\n");
            }
        }

        sc.close();
    }

}
